{
  "name": "ionic-app",
  "version": "1.0.0",
  "private": true,
  "description": "An Ionic React project",
  "author": {
    "name": "Luis Alejandro Mart√≠nez Faneyth",
    "email": "luis@luisalejandro.org"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LuisAlejandro/ionic-react-electron-ts-docker-template.git"
  },
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "electron:start-live": "node ./live-runner.js",
    "electron:start": "yarn run build && electron --inspect=5858 ./",
    "electron:pack": "yarn run build && electron-builder build --dir -c ./electron-builder.config.json",
    "electron:make": "yarn run build && electron-builder build -c ./electron-builder.config.json -p always",
    "electron:build-windows": "yarn run build && electron-builder build -c ./electron-builder.config.json --windows",
    "electron:build-mac": "yarn run build && electron-builder build -c ./electron-builder.config.json --mac",
    "electron:build-linux": "yarn run build && electron-builder build -c ./electron-builder.config.json --linux",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.0",
    "chokidar": "~3.5.3",
    "electron-is-dev": "~2.0.0",
    "electron-serve": "~1.1.0",
    "electron-unhandled": "~4.0.1",
    "electron-updater": "~4.6.5",
    "electron-window-state": "~5.0.3"
  },
  "devDependencies": {
    "electron": "^17.1.2",
    "electron-builder": "~22.14.13",
    "typescript": "~4.6.2"
  },
  "keywords": [
    "capacitor",
    "electron"
  ]
}
